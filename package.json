{
  "name": "@f5devcentral/f5-fast-core",
  "version": "0.6.0",
  "author": "F5 Networks",
  "license": "Apache-2.0",
  "main": "index.js",
  "description": "The core module for F5 Application Services Templates",
  "scripts": {
    "test": "mocha --recursive \"./test/*.js\"",
    "lint": "eslint lib test index.js cli.js --ignore-pattern jsoneditor.js",
    "buildbin": "./scripts/build-fastbin.sh",
    "coverage": "nyc npm test"
  },
  "keywords": [
    "as3",
    "f5",
    "rest",
    "api"
  ],
  "bin": {
    "fast": "./cli.js"
  },
  "devDependencies": {
    "@f5devcentral/eslint-config-f5-atg": "^0.1.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
    "mocha": "^7.2.0",
    "nock": "^12.0.3",
    "nyc": "^15.1.0",
    "pkg": "^4.4.9"
  },
  "eslintConfig": {
    "extends": "@f5devcentral/eslint-config-f5-atg",
    "rules": {
      "no-control-regex": "off"
    },
    "parserOptions": {
      "ecmaVersion": 8
    }
  },
  "nyc": {
    "all": true,
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "lib/jsoneditor.js"
    ]
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.1",
    "@f5devcentral/atg-storage": "^0.1.0",
    "ajv": "^6.12.2",
    "archiver": "^4.0.1",
    "deepmerge": "^4.2.2",
    "js-yaml": "^3.14.0",
    "mustache": "^4.0.1",
    "uuid": "^7.0.3",
    "yargs": "^15.4.0"
  }
}
